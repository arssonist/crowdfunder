<div class="container">

<h3>Create an Account</h3>


<%= form_for @user do |f| %>

 <div class='field'>
   <%= f.label :name%>
   <%= f.text_field :name%>
   </div>

   <div class='field'>
     <%= f.label :email %>
     <%= f.email_field :email %>
    </div>

      <%= f.submit %>
      <% end %>

</div>
